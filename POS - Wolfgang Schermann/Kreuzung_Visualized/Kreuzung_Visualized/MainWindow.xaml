        <Label Content="Sueden" HorizontalAlignment="Left" Margin="175,274,0,0" VerticalAlignment="Top" Width="70"/>
        <Label Content="Westen" HorizontalAlignment="Left" Margin="45,132,0,0" VerticalAlignment="Top" Width="70"/>
