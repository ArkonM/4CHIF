<Window x:Class="WPF_SQL_Datenbank.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_SQL_Datenbank"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label Content="Jahr" HorizontalAlignment="Left" Margin="40,29,0,0" VerticalAlignment="Top" Width="80"/>
        <TextBox x:Name="TB_year" HorizontalAlignment="Left" Height="23" Margin="40,55,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="280"/>
        <Label Content="Titel" HorizontalAlignment="Left" Margin="40,86,0,0" VerticalAlignment="Top" Width="80"/>
        <TextBox x:Name="TB_title" HorizontalAlignment="Left" Height="23" Margin="40,112,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="280"/>
        <Button x:Name="Btn_Submit" Content="Submit" HorizontalAlignment="Left" Margin="40,150,0,0" VerticalAlignment="Top" Width="75" Click="Btn_Submit_Click"/>
        <ListBox x:Name="LB_Books" HorizontalAlignment="Left" Height="140" Margin="380,29,0,0" VerticalAlignment="Top" Width="365"/>

    </Grid>
</Window>
