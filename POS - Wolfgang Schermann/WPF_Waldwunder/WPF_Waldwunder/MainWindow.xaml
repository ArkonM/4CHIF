        <TextBox x:Name="TXB_Type" HorizontalAlignment="Left" Height="23" Margin="25,266,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Typ" HorizontalAlignment="Left" Margin="25,239,0,0" VerticalAlignment="Top" Width="120"/>

    </Grid>
</Window>
